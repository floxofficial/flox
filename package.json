{
  "name": "flox",
  "version": "0.0.2",
  "description": "Flox",
  "main": "build/electron.js",
  "scripts": {
    "start": "electron .",
    "predeploy": "npm run prod",
    "dev": "cross-env NODE_ENV=development gulp build",
    "prod": "cross-env NODE_ENV=production gulp build",
    "package:macos": "./node_modules/.bin/electron-packager ./ Flox --platform=darwin --asar --arch=x64 --icon=./src/assets/logo/Flox-icon-256.icns --out=dist --overwrite",
    "package:linux": "./node_modules/.bin/electron-packager ./ Flox --platform=linux --asar --arch=x64 --icon=./src/assets/logo/Flox-icon-256.png --out=dist --overwrite",
    "package:windows": "electron-packager . Flox --platform=win32 --asar --arch=x64 --icon=./src/assets/logo/Flox-icon-256.ico --out=dist --overwrite",
    "build:dmg": "./node_modules/.bin/electron-installer-dmg ./dist/Flox-darwin-x64/flox.app Flox --icon=./src/assets/logo/Flox-icon-256.icns --out=./dist/installers/ --title=flox --overwrite",
    "build:deb": "./node_modules/.bin/electron-installer-debian --arch amd64 --config ./packager/deb.json",
    "build:rpm": "./node_modules/.bin/electron-installer-redhat --src ./dist/Flox-linux-x64 --arch x86_64 --config ./packager/rpm.json",
    "build:flatpak": "./node_modules/.bin/electron-installer-flatpak --src ./dist/Flox-linux-x64 --arch x86_64 --config ./packager/flatpak.json",
    "build:snap": "./node_modules/.bin/electron-installer-snap --src=./dist/Flox-linux-x64 --name Flox --dest=./dist/installer",
    "build:windows": "node packager/win.js"
  },
  "keywords": [],
  "author": "Matin Kaboli <matinkaboli79@gmail.com>",
  "license": "CC0-1.0",
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/runtime": "^7.3.1",
    "aes256": "^1.1.0",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "coingecko-api": "^1.0.10",
    "electron": "^5.0.6",
    "electron-squirrel-startup": "^1.0.0",
    "final-form": "^4.20.1",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.1",
    "history": "^4.7.2",
    "js-conflux-sdk": "^1.5.11",
    "lottie-react": "^2.1.0",
    "lottie-web": "^5.7.8",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "qrcode": "^1.3.3",
    "qrcode.react": "^1.0.0",
    "react": "^16.6.3",
    "react-bootstrap": "^1.4.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.6.3",
    "react-dropzone": "^11.2.4",
    "react-final-form": "^6.5.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.14",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "uuid": "^3.3.2",
    "webpack": "^4.25.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "del": "^3.0.0",
    "electron-winstaller": "^5.0.0",
    "file-loader": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.0.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2"
  }
}
