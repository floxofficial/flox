{
  "name": "Fullet",
  "version": "0.0.1",
  "description": "Fullet",
  "main": "build/electron.js",
  "scripts": {
    "start": "electron .",
    "predeploy": "npm run prod",
    "deploy": "./node_modules/.bin/gh-pages -d build",
    "dev": "cross-env NODE_ENV=development gulp dev",
    "prod": "cross-env NODE_ENV=production ./node_modules/.bin/gulp prod",
    "package:macos": "npm run predeploy; ./node_modules/.bin/electron-packager ./ Fullet --platform=darwin --asar --arch=x64 --icon=./src/images/icons/256x256.icns --out=dist --overwrite",
    "package:linux": "npm run predeploy; ./node_modules/.bin/electron-packager ./ Fullet --platform=linux --asar --arch=x64 --icon=./src/images/icons/256x256.png --out=dist --overwrite",
    "package:windows": "npm run predeploy; ./node_modules/.bin/electron-packager ./ Fullet --platform=win32 --asar --arch=x64 --icon=./src/images/icons/256x256.ico --out=dist --overwrite",
    "build:dmg": "./node_modules/.bin/electron-installer-dmg ./dist/Fullet-darwin-x64/Fullet.app Fullet --icon=./src/images/icons/256x256.icns --out=./dist/installers/ --title=Fullet --overwrite",
    "build:deb": "./node_modules/.bin/electron-installer-debian --arch amd64 --config ./packager/deb.json",
    "build:rpm": "./node_modules/.bin/electron-installer-redhat --src ./dist/Fullet-linux-x64 --arch x86_64 --config ./packager/rpm.json",
    "build:flatpak": "./node_modules/.bin/electron-installer-flatpak --src ./dist/Fullet-linux-x64 --arch x86_64 --config ./packager/flatpak.json",
    "build:snap": "./node_modules/.bin/electron-installer-snap --src=./dist/Fullet-linux-x64 --name Fullet --dest=./dist/installer",
    "build:windows": "nodejs packager/win.js",
    "release:dmg": "npm run package:macos; npm run build:dmg",
    "release:deb": "npm run package:linux; npm run build:deb",
    "release:rpm": "npm run package:linux; npm run build:rpm",
    "release:flatpak": "npm run package:linux; npm run build:flatpak",
    "release:snap": "npm run package:linux; npm run build:snap",
    "release:windows": "npm run package:windows; npm run build:windows"
  },
  "keywords": [],
  "author": "Matin Kaboli <matinkaboli79@gmail.com>",
  "license": "CC0-1.0",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "electron-squirrel-startup": "^1.0.0",
    "final-form": "^4.20.1",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.1",
    "history": "^4.7.2",
    "moment": "^2.24.0",
    "node-fetch": "^2.3.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "qrcode": "^1.3.3",
    "react": "^16.6.3",
    "react-bootstrap": "^1.4.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.6.3",
    "react-dropzone": "^11.2.4",
    "react-final-form": "^6.5.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.14",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "uuid": "^3.3.2",
    "webpack": "^4.25.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "del": "^3.0.0",
    "electron": "^5.0.6",
    "electron-installer-dmg": "^2.0.0",
    "electron-installer-snap": "^3.1.1",
    "electron-packager": "^13.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-eslint": "^5.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  }
}
